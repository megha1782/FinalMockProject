<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" jcr:primaryType="nt:unstructured" jcr:title="Enter Header Elements" sling:resourceType="cq/gui/components/authoring/dialog" helpPath="https://www.adobe.com/go/aem_cmp_image_v2">
    <content jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <columns jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns" margin="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <column jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <bannerHead1
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="TEXT For First Banner"
                                    name="./bannerHead1"/>
                            <bannerLink1
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                    fieldLabel="Link For First Banner "
                                    name="./bannerLink1"
                                    target="_blank"
                                    rootPath="/content"
                                    suffix=".html"/>

                            <image1
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                    fieldLabel="Link to image1"
                                    name="./image1"
                                    rootPath="/content"
                                    suffix=".html"/>
                            <bannerHead2
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="TEXT For Second Banner"
                                    name="./bannerHead2"/>
                            <bannerLink2
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                    fieldLabel="Link For Second Banner "
                                    name="./bannerLink2"
                                    target="_blank"
                                    rootPath="/content"
                                    suffix=".html"/>

                            <image2
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                    fieldLabel="Link to image2"
                                    name="./image2"
                                    rootPath="/content"
                                    suffix=".html"/>
                            <bannerHead3
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="TEXT For Third Banner"
                                    name="./bannerHead2"/>
                            <bannerLink3
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                    fieldLabel="Link For Third Banner "
                                    name="./bannerLink3"
                                    target="_blank"
                                    rootPath="/content"
                                    suffix=".html"/>

                            <image3
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                    fieldLabel="Link to image3"
                                    name="./image3"
                                    rootPath="/content"
                                    suffix=".html"/>

                        </items>
                    </column>
                </items>
            </columns>
        </items>
    </content>
</jcr:root>